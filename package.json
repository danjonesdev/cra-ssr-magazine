{
  "name": "cra-ssr",
  "homepage": "https://cra-ssr.herokuapp.com",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@sanity/block-content-to-react": "^2.0.2",
    "@sanity/client": "^0.132.5",
    "@sanity/image-url": "^0.132.5",
    "classnames": "^2.2.6",
    "connected-react-router": "^4.3.0",
    "cookie-parser": "^1.4.3",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "forcedomain": "^1.0.0",
    "husky": "^0.14.3",
    "ignore-styles": "^5.0.1",
    "js-cookie": "^2.2.0",
    "lint-staged": "^7.2.2",
    "md5-file": "^4.0.0",
    "morgan": "^1.9.0",
    "node-sass-chokidar": "^1.3.3",
    "prettier": "^1.14.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-facebook": "^5.0.3",
    "react-flickity-component": "^3.0.3",
    "react-frontload": "^1.0.1",
    "react-helmet": "^5.2.0",
    "react-iframe": "^1.2.0",
    "react-lazyload": "^2.3.0",
    "react-loadable": "^5.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "react-spotify-player": "^1.0.4",
    "react-youtube": "^7.6.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "eslint --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ],
    "src/**/*.{css}": [
      "stylelint --config=.stylelintrc --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:lint:js": "eslint --ext=js --ext=jsx .",
    "eject": "react-scripts eject",
    "serve": "NODE_ENV=production node ./server/index.js",
    "buildserve": "npm run build && npm run serve"
  },
  "devDependencies": {
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0"
  }
}
